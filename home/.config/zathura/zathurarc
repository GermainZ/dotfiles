# gruvbox color scheme
set recolor-darkcolor "#ebdbb2"
set recolor-lightcolor "#1d2021"
set recolor true
set recolor-keephue true
set completion-fg "#ebdbb2"
set completion-bg "#1d2021"
set completion-highlight-fg "#ebdbb2"
set completion-highlight-bg "#3c3836"
set default-fg "#ebdbb2"
set default-bg "#1d2021"
set inputbar-fg "#ebdbb2"
set inputbar-bg "#1d2021"
set statusbar-fg "#ebdbb2"
set statusbar-bg "#1d2021"
set render-loading-fg "#ebdbb2"
set render-loading-bg "#1d2021"
map c set recolor-keephue
